<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee" version="2.5">
    <servlet>
        <servlet-name>MyServlet</servlet-name>
        <servlet-class>com.example.DKTIQUE.myapplication.backend.MyServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MyServlet</servlet-name>
        <url-pattern>/hello</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>getProspectServlet</servlet-name>
        <servlet-class>com.example.DKTIQUE.myapplication.backend.getProspectServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>getProspectServlet</servlet-name>
        <url-pattern>/getProspect</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>AddProspectServlet</servlet-name>
        <servlet-class>com.example.DKTIQUE.myapplication.backend.AddProspectServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AddProspectServlet</servlet-name>
        <url-pattern>/addProspect</url-pattern>
    </servlet-mapping>







    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
</web-app>